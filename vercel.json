{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./frontend",
        "use": "@vercel/static-build",
        "config": { "buildCommand": "npm install && npm run build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./server.js"
      }
    ]
}
  
  